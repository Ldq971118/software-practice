<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >


<mapper namespace="com.softwarepractice.dao.DeleteInterface">

    <delete id="DeleteWorkerById" parameterType="Integer">
        DELETE FROM worker WHERE id=#{id}
    </delete>

    <delete id="DeleteAccommendation" parameterType="Integer">
        DELETE FROM accommendation WHERE id=#{id}
    </delete>

    <delete id="DeletePush" parameterType="Integer">
        DELETE FROM push WHERE id=#{id}
    </delete>


</mapper>